         <span class="active-wedding-name">Check Out
           </span>
     </div>
<br>
<%#= if FileTest.exist?("foto/#{@find_guest[0].guest_id}.jpg")
  image_tag("foto/#{@find_guest[0].guest_id}.jpg", style:'height:354.5px;')
  else
  render 'hadir-svg' end%>
<div style="padding-top:20px;">
      <% if File.file?("app/assets/images/foto/#{@find_guest[0].guest_id}.jpeg") %>
      <%= image_tag("foto/#{@find_guest[0].guest_id}.jpg", style:'height:354.5px;') %>
      <% else %>
      <%= render 'hadir-svg' %>
      <% end %>
</div>

<div class="result-text">
  <% if @find_guest[0].guest_id != nil %><p><span class="address"><%= @find_guest[0].guest_id %></span></p><% end %>
